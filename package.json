{
  "name": "cybersport-front",
  "version": "0.0.0",
  "description": "Frontend for cybersport.ru",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "nuxt-ts build",
    "dev": "nuxt-ts",
    "dev:mock": "cross-env MOCK=true yarn dev",
    "fix-editorjs": "cp -R .editorjs/* node_modules/@editorjs/editorjs/types/api/",
    "generate": "cross-env HYGEN_TMPLS=.templates hygen",
    "generate:component": "yarn generate component new --name",
    "generate:page": "yarn generate page new --name",
    "lint:script": "eslint --ext .ts,.js,.vue .",
    "lint:style": "stylelint src/**/*.scss",
    "lint": "yarn lint:script && yarn lint:style",
    "lint:fix": "yarn lint:script --fix && yarn lint:style --fix",
    "postinstall": "yarn fix-editorjs",
    "start": "nuxt-ts start",
    "storybook:dev": "start-storybook -p 9090 --ci",
    "storybook:build": "build-storybook -o .storybook-build",
    "test": "jest --no-cache",
    "test:coverage": "yarn test --coverage --colors --silent"
  },
  "dependencies": {
    "@cybersport/specifications": "^0.0.42",
    "@editorjs/editorjs": "^2.18.0",
    "@editorjs/header": "^2.5.0",
    "@editorjs/list": "^1.5.0",
    "@editorjs/marker": "^1.2.2",
    "@editorjs/simple-image": "^1.3.3",
    "@nuxtjs/axios": "^5.12.3",
    "cookie-universal-nuxt": "^2.1.3",
    "dayjs": "^1.8.28",
    "incstr": "^1.2.3",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.15",
    "mediaquerysensor": "^2.0.1",
    "normalize.css": "^8.0.1",
    "nuxt": "^2.12.2",
    "nuxt-property-decorator": "^2.7.2",
    "plural-ru": "^2.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.10.0",
    "@babel/plugin-transform-runtime": "^7.10.0",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@nuxt/typescript-build": "^0.6.6",
    "@nuxt/typescript-runtime": "^0.4.6",
    "@nuxtjs/color-mode": "^1.0.0",
    "@nuxtjs/device": "^1.2.7",
    "@nuxtjs/eslint-config-typescript": "^2.0.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/style-resources": "^1.0.0",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/vue": "^5.3.19",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.153",
    "@types/lowdb": "^1.0.9",
    "@types/multer": "^1.4.3",
    "@types/node": "^14.0.1",
    "@vue/test-utils": "^1.0.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "babel-preset-vue": "^2.0.2",
    "core-js": "3",
    "cross-env": "^7.0.2",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "express": "^4.17.1",
    "express-openapi-validator": "^3.16.7",
    "fork-ts-checker-webpack-plugin": "^4.1.6",
    "husky": "^4.2.5",
    "hygen": "^5.0.3",
    "jest": "<26.0.0",
    "lint-staged": "^10.2.2",
    "lowdb": "^1.0.0",
    "node-sass": "^4.14.1",
    "npm-cli-login": "^0.1.1",
    "postcss-pxtorem": "^5.1.1",
    "prettier": "^2.0.5",
    "sass-loader": "^8.0.2",
    "storybook": "^5.3.19",
    "stylelint": "^13.4.0",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "ts-jest": "^25.5.1",
    "ts-loader": "^7.0.4",
    "typescript": "^3.9.2",
    "vue-jest": "^3.0.5",
    "vue-svg-loader": "^0.16.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.scss": [
      "yarn lint:style --fix"
    ],
    "*.{ts,vue}": [
      "yarn lint:script --fix"
    ]
  }
}
